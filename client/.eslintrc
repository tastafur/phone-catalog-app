{
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "commonjs": true,
    "mocha": true
  },
  "globals": {
    "browser": true,
    "expect": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "rules": {
    "react/display-name": 0
  }
}
